{
  "manifest_version": 3,
  "name": "A.I. Prime",
  "version": "1.0.0",
  "description": "AI-powered browsing assistant that automates web navigation and tasks using natural language instructions",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": ["data-collection.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
